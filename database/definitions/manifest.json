            "Effect": "Allow",
            "Action": "dms:*",
            "Resource": "arn:aws:dms:eu-west-3:637423496590:<resource-path>"
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": "rds:*",
            "Resource": "arn:aws:rds:eu-west-3:637423496590:<resource-path>"
        },
        {
            "Sid": "VisualEditor3",
            "Effect": "Allow",
            "Action": "s3:GetObject",
