// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model prices_history {
    auctionHouseId Int?
    itemId         Int
    petSpeciesId   Int?
    minBuyout      Int?
    quantity       Int?
    marketValue    Int?
    historical     Int?
    numAuctions    Int?
    snapshotDate   String

    @@map("prices_history")
    @@id([itemId, snapshotDate])
}

model item_names {
  itemId   Int    @id
  itemName String?
  
  @@map("item_names")
}
